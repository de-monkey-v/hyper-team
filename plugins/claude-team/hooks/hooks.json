{
  "description": "Team cleanup on session end",
  "hooks": {
    "SessionEnd": [
      {
        "matcher": "*",
        "hooks": [
          {
            "type": "command",
            "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/cleanup-on-session-end.sh",
            "timeout": 30
          }
        ]
      }
    ]
  }
}
